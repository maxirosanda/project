{{> navbar}}
<h1>Pedido en proceso con numero de indentificacion:{{_id}}</h1>
{{#each items}}

     <div class="card mb-3 mt-5">
  <img src="../../img/products/{{url}}" class="card-img-top "  style="width: 25%" alt="...">
  <div class="card-body">
    <h5 class="card-title">{{name}}</h5>
   <p>Descripcion: {{description}}</p>
